{
  "name": "@lucidbyte/lucidbyte-js",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "lucidbyte",
  "version": "0.1.1",
  "main": "dist/lucidbyte.js",
  "scripts": {
    "start:dev": "NODE_ENV=development webpack-dev-server && npm run watchBuild",
    "watchBuild": "NODE_ENV=development webpack --watch",
    "dependencyStats": "webpack --profile --json > webpack-stats.json",
    "prepare": "NODE_ENV=production webpack",
    "productionTest": "NODE_ENV=production webpack --watch",
    "test": "NODE_ENV=test jest --watchAll"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-wrap-in-js": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "bulma": "^0.6.1",
    "classnames": "^2.2.5",
    "debounce": "^1.1.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "http-server": "^0.10.0",
    "query-string": "^5.0.1",
    "shortid": "^2.2.8",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "preact": "^8.2.7",
    "regenerator-runtime": "^0.11.1",
    "shortid": "^2.2.8"
  },
  "description": "sdk for lucidbyte frontend database",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leland-Kwong/lucidbyte-js.git"
  },
